			package hhhyyy;

			public class classe_do_xesque {
					public static void main(String[]args) {

						int total = 213;
						int aux = total;
						int numero1 = 0;
						int numero2 = 0;
						int base = 8;
						
						while (total >= base) {
							
							if (aux / base > 1) {
								numero1 ++;
							}
							else {
								numero2 = numero1 / base;
								numero1 = numero1 % base;
								if (numero2 == 0) {
								System.out.println(numero1+1);
								System.out.println(total-base);
								}
								else {
									System.out.println(numero2);
									System.out.println(numero1+1);
									System.out.println(total-base);
								}
							}
							total = aux-base;
							aux = total;
						}
						}							
		}
			

		




